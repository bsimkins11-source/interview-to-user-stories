# Production Environment Configuration
ENVIRONMENT=production

# GCP Project Configuration
GCP_PROJECT_ID=interview-to-user-stories
GCP_REGION=us-central1

# Service Account
GOOGLE_APPLICATION_CREDENTIALS=service-account-key.json

# Firestore Configuration
FIRESTORE_PROJECT_ID=interview-to-user-stories
FIRESTORE_DATABASE_ID=(default)

# Cloud Storage Configuration
STORAGE_BUCKET_NAME=interview-to-user-stories-data
STORAGE_PROJECT_ID=interview-to-user-stories

# Pub/Sub Configuration
PUBSUB_PROJECT_ID=interview-to-user-stories
PUBSUB_TOPIC_NAME=interview-processing-jobs
PUBSUB_SUBSCRIPTION_NAME=interview-processing-subscription

# AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# CORS Configuration
CORS_ALLOW_ORIGINS=https://your-frontend-domain.vercel.app,http://localhost:3000

# Security
SECRET_KEY=your_secret_key_here
